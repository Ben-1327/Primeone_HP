<!DOCTYPE html>
<html>
  <head>
    <title>PrimeoneHp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA6oI97dPNTskpK8GG5eU-T9ijOmLNAKG4&callback=initMap" async defer></script>
  </head>

  <body>
    <header class="navbar">
      <div class="container">
        <nav class="logo_space">
          <%= link_to root_path do %>
            <%= image_tag "logo_01.png", :width => "250", :height => "65" %>
            <% end %>
        </nav>
        <nav class="nav">
          <ul class="nav p-4">
            <li class="nav-item mr-3"><%= link_to "Home", root_path, class: "btn btn-nav" %></li>
            <li class="nav-item mr-3"><%= link_to "About", about_path, class: "btn btn-nav" %></li>
            <li class="nav-item mr-3"><%= link_to "Contact", contact_path, class: "btn btn-nav" %></li>
            <li class="nav-item"><%= link_to "Access", access_path, class: "btn btn-nav" %></li>
          </ul>
        </nav>
      </div>
    </header>
    <%= yield %>
  </body>
</html>
